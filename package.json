{
  "name": "soccertrackerapp",
  "version": "1.0.0",
  "private": true,
  "description": "A nwjs-vue application",
  "author": "SushrutPola, PrajwalVandana",
  "scripts": {
    "build": "tsc && node .nwjs-vue/prod-builder.js",
    "dev": "tsc && node .nwjs-vue/dev-runner.js"
  },
  "main": "http://localhost:9080",
  "dependencies": {
    "node-gyp": "^8.2.0",
    "vue-class-component": "^7.2.3",
    "vue-property-decorator": "^9.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-service": "^4.5.13",
    "ajv": "^6.10.2",
    "axios": "^0.19.0",
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.2.0",
    "file-loader": "^4.2.0",
    "fs-extra": "^8.1.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "npm-which": "^3.0.1",
    "nwjs-builder-phoenix": "^1.15.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "sass-loader": "^7.3.1",
    "typescript": "~4.1.5",
    "url-loader": "^2.1.0",
    "vue": "^2.6.10",
    "vue-loader": "^15.7.1",
    "vue-router": "^3.1.2",
    "vue-template-compiler": "^2.6.10",
    "vuex": "^3.1.1",
    "webpack": "^4.39.2",
    "webpack-dev-server": "^3.8.0",
    "webpack-hot-middleware": "^2.25.0"
  },
  "bg-script": "./dist/bg/bg.js",
  "build": {
    "files": [
      "node_modules/**/*",
      "dist/**/*"
    ],
    "output": "./releases",
    "nwVersion": "0.40.1",
    "nwPlatforms": [
      "win"
    ],
    "nwArchs": [
      "x64"
    ],
    "strippedProperties": [
      "label",
      "private",
      "scripts",
      "devDependencies",
      "build"
    ],
    "overriddenProperties": {
      "main": "./dist/main/index.html"
    }
  },
  "label": "Soccertrackerapp",
  "node-remote": "*://localhost/*",
  "window": {
    "width": 1000,
    "height": 563
  }
}
